{
  "version": 2,
  "buildCommand": "npm ci && npm run vercel-build",
  "outputDirectory": "client/build",
  "functions": {
    "server/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server/index.js"
    }
  ]
}